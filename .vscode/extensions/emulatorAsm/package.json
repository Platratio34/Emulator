{
    "name": "emulatorasm",
    "displayName": "Emulator ASM Support",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.103.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:plaintext"
    ],
    "main": "./client/out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "emulatorasm",
                "extensions": [
                    ".asm"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "emulatorlang",
                "extensions": [
                    ".el"
                ],
                "configuration": "./el-language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "emulatorasm",
                "scopeName": "source.asm",
                "path": "./syntaxes/asm.tmGrammar.json"
            },
            {
                "language": "emulatorlang",
                "scopeName": "source.el",
                "path": "./syntaxes/el.tmGrammar.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "22.x",
        "@types/vscode": "^1.103.0",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "@typescript-eslint/parser": "^8.39.0",
        "@vscode/test-cli": "^0.0.11",
        "@vscode/test-electron": "^2.5.2",
        "eslint": "^9.32.0",
        "typescript": "^5.9.2"
    },
    "dependencies": {
        "emulatorasm": "file:",
        "vscode-languageserver": "^9.0.1"
    }
}
